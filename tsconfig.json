{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "noImplicitAny": false,
    "noStrictGenericChecks": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-ioform": [
        "projects/lib/src"
      ],
      "ngx-ioform/*": [
        "projects/lib/src/*"
      ],
      "io-form": [
        "dist/io-form",
        "dist/io-form"
      ],
      "io-form/*": [
        "dist/io-form/*",
        "dist/io-form/*"
      ]
    }
  }
}